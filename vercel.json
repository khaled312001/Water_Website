{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "vercel-php@0.6.0"
    },
    {
      "src": "api/test.php",
      "use": "vercel-php@0.6.0"
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "/api/test.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
} 